zshrc                       ~/.zshrc
init.vim                    ~/.config/nvim/init.vim
vim/*                       ~/.config/nvim/
tmux.conf                   ~/.tmux.conf
tmux.remote.conf            ~/.tmux/tmux.remote.conf
gitignore_global            ~/.config/git/ignore
gitconfig.shared            ~/.gitconfig.shared
themes/*.vim                ~/.config/nvim/colors/
themes/*.zsh-theme          ~/.oh-my-zsh/themes/
